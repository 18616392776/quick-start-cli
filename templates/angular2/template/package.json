{
  "name": "angular2-webpack",
  "version": "1.0.0",
  "description": "A webpack starter for Angular",
  "main": "server/server.js",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development node server/server.js",
    "doc": "cross-env NODE_ENV=documentation node server/doc-server.js",
    "pack": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config config/webpack.prod.js --progress --profile --bail --optimize-minimize"
  },
  "keywords": [
    "angular2",
    "ng2",
    "typescript",
    "ts",
    "webpack",
    "es6"
  ],
  "author": "tanbo",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.4.9",
    "@angular/compiler": "2.4.9",
    "@angular/core": "2.4.9",
    "@angular/forms": "2.4.9",
    "@angular/http": "2.4.9",
    "@angular/platform-browser": "2.4.9",
    "@angular/platform-browser-dynamic": "2.4.9",
    "@angular/router": "3.4.9",
    "core-js": "2.4.1",
    "normalize.css": "^5.0.0",
    "rxjs": "5.2.0",
    "zone.js": "0.7.8",
    "bootstrap": "3.3.7"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.40",
    "@types/node": "6.0.60",
    "angular2-template-loader": "0.5.0",
    "autoprefixer": "6.6.1",
    "awesome-typescript-loader": "2.2.4",
    "codelyzer": "1.0.0-beta.4",
    "connect-history-api-fallback": "1.3.0",
    "cross-env": "3.1.4",
    "css-loader": "0.25.0",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "highlight.js": "9.9.0",
    "html-loader": "0.4.4",
    "html-minify-loader": "1.1.0",
    "html-webpack-plugin": "2.26.0",
    "http-proxy-middleware": "0.17.3",
    "ip": "1.1.4",
    "jasmine-core": "2.5.2",
    "json-loader": "0.5.4",
    "karma": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.1",
    "markdown-it": "8.3.0",
    "null-loader": "0.1.1",
    "open": "0.0.5",
    "phantomjs-prebuilt": "2.1.14",
    "postcss": "5.2.9",
    "postcss-loader": "0.13.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "to-string-loader": "1.1.5",
    "tslint": "3.15.1",
    "tslint-formatter-eslint-style": "0.0.1",
    "tslint-loader": "2.1.5",
    "typescript": "2.0.10",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-middleware": "1.9.0",
    "webpack-dev-server": "1.16.2",
    "webpack-hot-middleware": "2.15.0",
    "webpack-merge": "0.14.1"
