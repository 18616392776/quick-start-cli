{
  "name": "vue-starter",
  "version": "1.0.0",
  "description": "A webpack starter for Vue2",
  "main": "server/server.js",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development node server/server.js",
    "pack": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail --optimize-minimize",
    "production": "npm run build && node server/product-server.js"
  },
  "keywords": [
    "vue2",
    "webpack",
    "es6"
  ],
  "author": "",
  "dependencies": {
    "normalize.css": "~8.0.0",
    "vue": "2.6.11",
    "vue-router": "3.1.6"
  },
